datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  clerkId   String   @unique @db.VarChar(32)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
